{
  "name": "sqltools-intersystems-driver",
  "displayName": "SQLTools InterSystems IRIS",
  "description": "SQLTools Driver for InterSystems IRIS",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "publisher": "intersystems-community",
  "license": "MIT",
  "repository": {},
  "bugs": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package -o sqltools-intersystems-driver.vsix",
    "lint": ""
  },
  "keywords": [
    "intersystems-iris-driver",
    "intersystems",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "ls": "./out/ls/plugin.js",
  "dependencies": {
    "@sqltools/base-driver": "^0.1.8",
    "@sqltools/types": "^0.1.5",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.14",
    "@types/vscode": "^1.46.0",
    "typescript": "^3.7.3",
    "vsce": "^1.88.0"
  }
}
